<?xml version="1.0" encoding="utf-8"?>
<resources>
<integer name="ga_sessionTimeout">300</integer>

<!-- Enable automatic Activity measurement -->
<bool name="ga_autoActivityTracking">true</bool>

<!-- the Local LogLevel for Analytics -->
<string name="ga_logLevel">verbose</string>

<!--Hide user IPs from results-->
<string name="ga_anonymizeIp">true</string>

<!-- how often the dispatcher should fire -->
<integer name="ga_dispatchPeriod">30</integer>

<!-- catch and report uncaught exceptions from the app -->
<bool name="ga_reportUncaughtExceptions">true</bool>

<!-- Treat events as test events and don't send to google -->
<bool name="ga_dryRun">true</bool>

<integer name="ga_sessionTimeout">300</integer>

<!-- The screen names that will appear in reports -->
<screenName name="org.creativecommons.thelist.activities.MainActivity">Main Feed Activity</screenName>
<screenName name="org.creativecommons.thelist.activities.RandomActivity">Random Item Suggestions Activity</screenName>
<screenName name="org.creativecommons.thelist.activities.CategoryListActivity">Category List Activity</screenName>
<screenName name="org.creativecommons.thelist.activities.StartActivity">Start Activity</screenName>

<!--  The following value should be replaced with correct property id. -->
<string name="ga_trackingId">UA-2010376-31</string>
</resources>